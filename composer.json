{
  "name": "fmarin/financial",
  "license": "MIT",
  "type": "project",
  "description": "Financial Domain",
  "require": {
    "php": ">7.4",

    "ext-apcu": "*",
    "ext-json": "*",
    "ext-zend-opcache": "*",
    "ext-pdo": "*",

    "symfony/framework-bundle": "^4.4",
    "symfony/dotenv": "^4.4",
    "symfony/yaml": "^4.4"
  },
  "require-dev": {
    "ext-xdebug": "*",

    "symfony/debug": "^4.4",

    "roave/security-advisories": "dev-master",

    "behat/behat": "^3.5",
    "behat/mink-extension": "^2.3",
    "behat/mink-browserkit-driver": "^1.3",
    "friends-of-behat/symfony-extension": "^2.0",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Financial\\Apps\\Backoffice\\Backend\\": "apps/backoffice/backend/src",
      "Financial\\Apps\\Backoffice\\Frontend\\": "apps/backoffice/frontend/src",

      "Financial\\": ["src"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Financial\\Tests\\": ["tests/src"]
    }
  }
}
